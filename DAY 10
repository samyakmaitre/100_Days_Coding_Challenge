 class Solution {
    public int[] twoSum(int[] nums, int target)
    {
        int l=nums.length;
        int i;int k=0;int j;
        int a[]=new int[2];
        for(i=0;i<l-1;i++)
        {
        for(j=i+1;j<l;j++)
        {
            if(nums[i]+nums[j]==target)
            {
            a[0]=i;
            a[1]=j;
            }
         }
        }
        return a;
     }
}
